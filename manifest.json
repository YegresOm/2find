{
  "manifest_version": 2,

  "name": "Find and save",
  "description": "This extension find, save and show current song on radiotuna",
  "version": "1.0",
  
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
	  "js": ["jquery-1.10.1.min.js", "keymaster.min.js" ,"injected.js"]
    }
  ],
  
  "background": {
    "persistent": true,
    "scripts": ["jquery-1.10.1.min.js", "background.js"]
  },
  
  "permissions": [
     "https://*/*",
     "http://*/*",
	 "tabs"
  ]
}